{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NODE_ENV"],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", ".source/**", "build/**", "dist/**", "out/**"]
    },
    "clean": {
      "cache": false,
      "persistent": false
    },
    "clean:all": {
      "cache": false,
      "persistent": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "types:check": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
